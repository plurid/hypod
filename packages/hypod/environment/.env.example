ENV_MODE=local
NODE_ENV=development
PORT=56565


PLURID_BUILD_DIRECTORY=build
PLURID_DEFAULT_VERBOSE=false


HYPOD_QUITE=false
HYPOD_LOG_LEVEL=0 # 7 None, 6 Fatal, 5 Error, 4 Warn, 3 Info, 2 Debug, 1 Trace, 0 All


HYPOD_DATABASE_TYPE=filesystem
HYPOD_STORAGE_TYPE=filesystem
