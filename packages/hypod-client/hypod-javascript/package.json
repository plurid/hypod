{
    "name": "@plurid/hypod-client",
    "version": "0.0.0-0",
    "description": "Cloud Service Client for Imagene Registry",
    "keywords": [
        "hypod",
        "client"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "SEE LICENSE IN LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/plurid/hypod"
    },
    "bugs": {
        "email": "source@plurid.com",
        "url": "https://github.com/plurid/hypod/issues"
    },
    "homepage": "https://github.com/plurid/hypod/tree/master/packages/hypod-client/hypod-javascript",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "engines": {
        "node": ">=12",
        "npm": ">=6"
    },
    "main": "distribution/index.js",
    "module": "distribution/index.es.js",
    "typings": "distribution/index.d.ts",
    "files": [
        "distribution"
    ],
    "scripts": {
        "test": "jest -c ./configurations/jest.config.js --coverage ./source",
        "start": "rollup -c ./scripts/rollup.config.ts -w",
        "runner": "runner tests",
        "build.removeTests": "rm -rf `find ./distribution/ -type d -name __tests__`",
        "build.types": "ttsc --emitDeclarationOnly",
        "build.development": "rollup -c ./scripts/rollup.config.ts",
        "build.production": "rollup -c ./scripts/rollup.config.ts",
        "build": "pnpm build.production && pnpm build.types && pnpm build.removeTests",
        "prebuild": "rm -rf distribution",
        "prepublishOnly": "pnpm build"
    },
    "peerDependencies": {
        "@apollo/client": ">=3.5",
        "@plurid/plurid-functions": "*",
        "cross-fetch": ">=3.1",
        "graphql": ">=16.3"
    },
    "devDependencies": {
        "@apollo/client": "^3.5.10",
        "@plurid/plurid-functions": "0.0.0-21",
        "@plurid/runner": "^0.0.0-9",
        "@rollup/plugin-commonjs": "^21.0.2",
        "@rollup/plugin-node-resolve": "^13.1.3",
        "@types/jest": "^27.4.1",
        "@types/node": "^17.0.21",
        "@typescript-eslint/eslint-plugin": "^5.15.0",
        "@typescript-eslint/parser": "^5.15.0",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "cross-fetch": "^3.1.5",
        "eslint": "^8.11.0",
        "graphql": "^16.3.0",
        "jest": "^27.5.1",
        "jest-config": "^27.5.1",
        "react": "^17.0.2",
        "rollup": "^2.70.1",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.31.2",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.7.0",
        "ttypescript": "^1.5.13",
        "typescript": "^4.6.2",
        "typescript-transform-paths": "^3.3.1"
    }
}
